{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "license": "MIT",
  "scripts": {
    "bootstrap": "pnpm generate:core && pnpm build:core",
    "start": "dumi dev",
    "site": "dumi build",
    "preview": "dumi preview",
    "clean": "rimraf ./**/node_modules",
    "generate": "pnpm -r --workspace-concurrency=1 run generate",
    "generate:core": "pnpm -r --filter @sensoro-design/icons-svg run generate",
    "build": "pnpm -r --workspace-concurrency=1 run build",
    "build:clean": "pnpm -r --workspace-concurrency=1 run build:clean",
    "build:core": "pnpm -r --filter @sensoro-design/icons-svg run build",
    "release": "walrus release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sensoro-design/sensoro-design-icons.git"
  },
  "bugs": {
    "url": "https://github.com/sensoro-design/sensoro-design-icons/issues"
  },
  "homepage": "https://sen-icons.xingkang.wang",
  "devDependencies": {
    "@pansy/shared": "catalog:",
    "@rgrove/parse-xml": "^3.0.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@sensoro-design/icons": "workspace:*",
    "@sensoro-design/icons-svg": "workspace:*",
    "@types/gulp": "catalog:",
    "@types/gulp-concat": "catalog:",
    "@types/gulp-header": "catalog:",
    "@types/gulp-rename": "catalog:",
    "@types/lodash": "catalog:",
    "@types/ramda": "^0.26.43",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/svgo": "^2.6.3",
    "@types/through2": "^2.0.38",
    "@umijs/utils": "^4.0.53",
    "@walrus/cli": "^1.3.4",
    "@walrus/plugin-release": "^1.14.3",
    "antd": "catalog:",
    "conventional-changelog-gitmoji-config": "^1.4.7",
    "del": "^7.0.0",
    "dumi": "catalog:",
    "gulp": "catalog:",
    "gulp-concat": "catalog:",
    "gulp-header": "catalog:",
    "gulp-rename": "catalog:",
    "husky": "^8.0.3",
    "lerna": "^6.5.1",
    "lint-staged": "catalog:",
    "lodash": "catalog:",
    "prettier": "catalog:",
    "ramda": "0.27.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "father": "^4.4.0",
    "rimraf": "catalog:",
    "semantic-release": "^20.1.1",
    "semantic-release-monorepo": "^7.0.5",
    "svgo": "^2.8.0",
    "through2": "^4.0.2",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "use-clipboard-hook": "catalog:",
    "vinyl": "^2.2.1",
    "zx": "catalog:"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
